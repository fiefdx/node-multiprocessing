    "babel-eslint": "^7.1.1",
    "babel-preset-latest": "^6.22.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.15.0",
    "eslint-plugin-babel": "^4.0.1",
    "lodash": "^4.17.4",
    "mocha": "^5.2.0"
  }
