{
  "name": "multiprocessing",
  "version": "1.2.0",
  "description": "Dead simple parallel processing for node",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanpon/node-multiprocessing"
  },
  "main": "index.js",
  "scripts": {
